"Trail2D": ""
}

[application]

config/name="GWGJ51Upgrade"
run/main_scene="res://src/Main.tscn"
config/icon="res://icon.png"

[autoload]

Constants="*res://src/Constants.tscn"
SubPub="*res://src/SubPub.tscn"
Game="*res://src/Game.tscn"
SoundManager="*res://addons/sound_manager/sound_manager.tscn"

[debug]

gdscript/warnings/unused_variable=false
gdscript/warnings/unused_argument=false
gdscript/warnings/unused_signal=false
gdscript/warnings/return_value_discarded=false

[display]

mouse_cursor/custom_image="res://asset/icons/crosshair087.png"
mouse_cursor/custom_image_hotspot=Vector2( 36, 36 )
mouse_cursor/tooltip_position_offset=Vector2( -36, -36 )
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/ImportCleaner/plugin.cfg", "res://addons/sound_manager/plugin.cfg" )

