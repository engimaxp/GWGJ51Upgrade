"base": "Node2D",
"class": "BoostBase",
"language": "GDScript",
"path": "res://src/BoostBase.gd"
}, {
"base": "Reference",
"class": "CardStateBase",
"language": "GDScript",
"path": "res://src/card_state/CardStateBase.gd"
}, {
"base": "Node",
"class": "CoolDownProxy",
"language": "GDScript",
"path": "res://src/tank/CoolDownProxy.gd"
}, {
